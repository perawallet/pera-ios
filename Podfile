platform :ios, '11.0'

target 'algorand' do

	use_frameworks!
	
	#Layout
	pod 'SnapKit'

	#Tool
	pod 'SwiftLint'

	#Networking
	pod 'Magpie', :git => 'https://github.com/Hipo/magpie.git'

    #Persistance
    pod 'KeychainAccess'
    
    #Date
    pod 'SwiftDate'
    
    #Analytics
    pod 'Firebase/Core'
    pod 'Fabric'
    pod 'Crashlytics'
end
